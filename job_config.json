{
    "job": {
        "command": "qsub object_detection_job.sh -l nodes=1:NODE -F \" RESULTS TARGET  FP_MODEL /data/reference-sample-data/object-detection-python/cars_1900.mp4  N_REQ\" -v VENV_PATH,OPENVINO_RUNTIME",
